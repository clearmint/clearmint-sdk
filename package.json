{
  "name": "clearmint-sdk",
  "version": "0.0.0",
  "private": false,
  "type": "module",
  "description": "Clearmint SDK â€” Node.js infrastructure-grade Solana minting SDK",
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": ["dist"],
  "engines": { "node": ">=18" },
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
