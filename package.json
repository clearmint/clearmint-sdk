  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "eslint": "^8.50.0",
    "eslint-config-standard-with-typescript": "^35.0.0",
    "eslint-plugin-import": "^2.30.0",
    "eslint-plugin-n": "^16.0.0",
    "eslint-plugin-promise": "^7.1.0",
    "pnpm": "^7.0.0",
    "tsup": "^6.7.0",
    "typescript": "^5.2.2",
    "vitest": "^1.3.0",
    "@vitest/coverage-v8": "^1.6.1",
    "sinon": "^15.0.0",
    "@types/sinon": "^10.0.0"
